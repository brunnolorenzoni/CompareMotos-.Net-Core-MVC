@model IEnumerable<CompareMotos.Models.Electric>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ignition)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Start)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Battery)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ignition.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Start.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Battery.Name)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ElectricId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ElectricId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ElectricId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
