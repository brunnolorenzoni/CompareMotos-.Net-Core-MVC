@{
    ViewData["Title"] = "Index";
}

    <div class="dashboard">

        <h1 class="title">Cadastro de motos</h1>


        <section class="section">
            <h2 class="sub-title">Moto</h2>
            <ul class="nav-list">
                <li class="item">
                    <a asp-area="" asp-controller="Motorcycles" asp-action="Index">Cadastrar moto</a>
                </li>
            </ul>
        </section>

        <section class="section">
            <h2 class="sub-title">Tipo de moto</h2>
            <ul class="nav-list">
                <li class="item">
                    <a asp-area="" asp-controller="TypeMotorcycles" asp-action="Index">Cadastrar os tipos de moto</a>
                </li>
            </ul>
        </section>

        <section class="section">
            <h2 class="sub-title">Fabricantes</h2>
            <ul class="nav-list">
                <li class="item">
                    <a asp-area="" asp-controller="Brands" asp-action="Index">Cadastrar fabricantes</a>
                </li>
            </ul>
        </section>

        <section class="section">
            <h2 class="sub-title">Modelos de motos</h2>
            <ul class="nav-list">
                <li class="item">
                    <a asp-area="" asp-controller="ModelMotorcycles" asp-action="Index">Cadastrar modelo de moto</a>
                </li>
            </ul>
        </section>

        <section class="section">
            <h2 class="sub-title">Cilindradas</h2>
            <ul class="nav-list">
                <li class="item">
                    <a asp-area="" asp-controller="Displacements" asp-action="Index">Cadastrar cilindrada</a>
                </li>
            </ul>
        </section>


    </div>
